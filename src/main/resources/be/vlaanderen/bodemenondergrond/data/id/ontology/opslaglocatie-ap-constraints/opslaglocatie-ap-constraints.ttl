beo_ont:opslaglocatie
    a               owl:Ontology ;
    rdfs:comment
                    "Specifieert hoe de data in de codelijst van Opslaglocaties te encoderen in RDF (i.e. RDF termen, kardinaliteiten,...)."@en ;
    rdfs:label      "Opslaglocaties AP Constraints Limited"@en ;
    dct:abstract    "Het Opslaglocaties applicatieprofiel specifieert de RDF termen (klassen en properties) en bijhorende constraints die gebruikt worden voor het beschrijven van Opslaglocaties. Het maakt hierbij zoveel mogelijk gebruik van bestaande termen (klassen en properties) uit bestaande , skos, dbo, dbp en een minimale set van nieuwe termen gedefinieerd in het Opslaglocaties Vocabularium."@en ;
    dct:alternative "Opslaglocaties AP Shapes"@en ;
    dct:creator     beo_ag_cl:creator ;
    dct:description "Dit is de SHACL encodering van het applicatieprofiel van een Opslaglocaties dat de eerste stap van de transformatie vanuit csv controleert."@en ;
    dct:issued      "2023-10-23"^^xsd:date ;
    dct:title       "Opslaglocaties AP Constraints Limited"@en ;
    xkos:belongsTo  beo_dataset:codelijst-opslaglocatie ;
    xhv:last        beo_voc:opslaglocatie ;
    owl:sameAs      beo_voc:opslaglocatie ;
    owl:versionInfo "Versie 3.0.0  maart 2024"@en .


beo_voc:opslaglocatie
    a          owl:Ontology ;
    rdfs:label "Opslaglocatie AP Constraints"@en ;
    owl:sameAs beo_ont:opslaglocatie .

beo_ag_cl:creator
    rdfs:label "Ontwikkelaar codelijst"@en ;
    schema:affiliation vl_org:OVO003749 ;
    a foaf:Person;
    foaf:firstName "Geert";
    foaf:lastName "Van Haute";
    foaf:mbox <mailto:geert.vanhaute@vlaanderen.be>.

# Node Shapes
beo_ns:schema
    a                sh:NodeShape ;
    rdfs:label       "conceptschema van opslaglocaties"@en ;
    sh:name          "conceptschema van opslaglocaties"@en ;
    sh:description   "Het schema waarbinnen de opslaglocaties gedefinieerd zijn"@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:property      beo_ps:conceptscheme_prefLabel, beo_ps:schema_hasTopConcept, beo_ps:dc_identifier, beo_ps:notation, beo_ps:note,
                     beo_ps:definition, beo_ps:rdf_type;
    sh:closed true ;
    sh:targetClass   skos:ConceptScheme .

beo_ns:collection
    a                sh:NodeShape ;
    rdfs:label       "verzameling"@en ;
    sh:name          "verzameling"@en ;
    sh:description   "Verzamelingen bundelen opslaglocaties met gelijkaardige eigenschappen, analysemethoden of opslaglocaties waarin ze gemeten worden."@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:property      beo_ps:prefLabel, beo_ps:collection_member, beo_ps:dc_identifier, beo_ps:notation,
                     beo_ps:inScheme, beo_ps:rdf_type;
    sh:closed true ;
    sh:targetClass   skos:Collection .

beo_ns:concept
    a                sh:NodeShape ;
    rdfs:label       "opslaglocaties van monsters"@en ;
    sh:name          "opslaglocaties van monsters"@en ;
    sh:description   "opslaglocaties van monsters"@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:property      beo_ps:notation, beo_ps:inScheme,
                     beo_ps:references, beo_ps:definition,
                     beo_ps:prefLabel, beo_ps:theme,
                     beo_ps:topConceptOf, beo_ps:altLabel, beo_ps:dc_identifier,
                     beo_ps:rdf_type, beo_ps:note ;
    sh:closed true ;
    sh:targetClass   skos:Concept .

# Property Shapes

beo_ps:rdf_type
    a                sh:PropertyShape ;
    rdfs:label       "rdf type"@en ;
    sh:name          "rdf type"@en ;
    sh:description   "Aanduiding dat een bron een instantie van een bepaalde klasse is."@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:maxCount      1 ;
    sh:minCount      1 ;
    sh:nodeKind      sh:IRI ;
    sh:path          rdf:type .

beo_ps:topConceptOf
    a                sh:PropertyShape ;
    rdfs:label       "topconcept van"@en ;
    sh:name          "topconcept van"@en ;
    sh:description   "De relatie tussen een concept en het schema, waarvan het een topconcept is."@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:class         skos:ConceptScheme ;
    sh:maxCount      1 ;
    sh:nodeKind      sh:IRI ;
    sh:path          skos:topConceptOf .

beo_ps:theme
    a                sh:PropertyShape ;
    rdfs:label       "thema"@en ;
    sh:name          "thema"@en ;
    sh:description   "De relatie naar een europees thema"@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:nodeKind      sh:IRI ;
    sh:path          dcat:theme .

beo_ps:narrower
    a                sh:PropertyShape ;
    rdfs:label       "specifieker concept"@en ;
    sh:name          "specifieker concept"@en ;
    sh:description   "De relatie tussen een concept en een nauwer, specifieker concept binnen hetzelfde schema."@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:class         skos:Concept ;
    sh:nodeKind      sh:IRI ;
    sh:path          skos:narrower .

beo_ps:broader
    a                sh:PropertyShape ;
    rdfs:label       "breder concept"@en ;
    sh:name          "breder concept"@en ;
    sh:description   "De relatie tussen een concept en een breder, minder specifiek concept binnen hetzelfde schema."@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:class         skos:Concept ;
    sh:maxCount      1 ;
    sh:nodeKind      sh:IRI ;
    sh:path          skos:broader .

beo_ps:topConceptOf
    a                sh:PropertyShape ;
    rdfs:label       "topconcept van"@en ;
    sh:name          "topconcept van"@en ;
    sh:description   "De relatie tussen een concept en het schema, waarvan het een topconcept is."@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:class         skos:ConceptScheme ;
    sh:maxCount      1 ;
    sh:nodeKind      sh:IRI ;
    sh:path          skos:topConceptOf .

beo_ps:altLabel
    a                sh:PropertyShape ;
    rdfs:label       "alternatief label"@en ;
    sh:name          "alternatief label"@en ;
    sh:description   "Een alternatief lexicaal label voor een bepaalde bron."@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:datatype      rdf:langString ;
    sh:languageIn    ( "nl" "en" ) ;
    sh:nodeKind      sh:Literal ;
    sh:path          skos:altLabel .

beo_ps:collection_member
    a                sh:PropertyShape ;
    rdfs:label       "onderdeel"@en ;
    sh:name          "onderdeel"@en ;
    sh:description   "Een collectie heeft minstens 1 onderdeel."@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:class         skos:Concept ;
    sh:minCount      1 ;
    sh:nodeKind      sh:IRI ;
    sh:path          skos:member .

beo_ps:exactMatch
    a                sh:PropertyShape ;
    rdfs:label       "heeft exacte overeenkomst"@en ;
    sh:name          "heeft exacte overeenkomst"@en ;
    sh:description   "skos:exactMatch wordt gebruikt om twee concepten te koppelen, wat aangeeft dat er een hoge mate van vertrouwen bestaat dat de concepten onderling uitwisselbaar kunnen worden gebruikt in een breed scala aan toepassingen voor het ophalen van informatie. skos:exactMatch is een transitieve eigenschap en is een subeigenschap van skos:closeMatch"@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:nodeKind      sh:IRI ;
    sh:path          skos:exactMatch .

beo_ps:notation
    a                sh:PropertyShape ;
    rdfs:label       "notatie"@en ;
    sh:name          "notatie"@en ;
    sh:description   "Een notatie, ook wel classificatiecode genoemd, is een reeks tekens zoals \"PCB151\" of \"EtFOSA\" die wordt gebruikt om een matrix op unieke wijze te identificeren binnen de context van dit schema."@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:datatype      xsd:string ;
    sh:nodeKind      sh:Literal ;
    sh:property      beo_ps:unique_notation ;
    sh:path          skos:notation .

beo_ps:unique_notation
    a                  sh:PropertyShape ;
    rdfs:label         "unieke notatie"@en ;
    sh:name            "unieke notatie"@en ;
    sh:description     "Een notatie, ook wel classificatiecode genoemd, is een reeks tekens zoals \"PCB151\" of \"EtFOSA\" die wordt gebruikt om een matrix op unieke wijze te identificeren binnen de context van dit schema. Er kan vanuit verschillende classificatiesystemen, met meerdere notaties verwezen worden naar een matrix, maar een notatie is uniek voor een matrix, ttz. vanuit 1 notatie kan slechts naar 1 matrix worden verwezen."@en ;
    rdfs:isDefinedBy   beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:nodeKind        sh:IRI ;
    sh:targetObjectsOf skos:notation ;
    sh:path            [ sh:inversePath skos:notation ] ;
    sh:minCount        1 ;
    sh:maxCount        1 .

beo_ps:schema_hasTopConcept
    a                sh:PropertyShape ;
    rdfs:label       "heeft topconcept"@en ;
    sh:name          "heeft topconcept"@en ;
    sh:description   "Verbindt een conceptschema met een concept dat bovenaan in een 'broader/narrower' hierarchie staat en voorziet aldus een ingang tot die hierarchie."@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:class         skos:Concept ;
    sh:minCount      1 ;
    sh:nodeKind      sh:IRI ;
    sh:path          skos:hasTopConcept .

beo_ps:references
    a                sh:PropertyShape ;
    rdfs:label       "wordt vermeld in"@en ;
    sh:name          "wordt vermeld in"@en ;
    sh:description   "Binnen dit schema wordt dct:references gebruikt om de relatie tussen analysemethodes en wat geanalyseerd wordt te leggen, die betrekking hebben op de matrix."@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:nodeKind      sh:IRI ;
    sh:path          dct:references .

beo_ps:definition
    a                sh:PropertyShape ;
    rdfs:label       "definitie"@en ;
    sh:name          "definitie"@en ;
    sh:description   "Een uitspraak over de betekenis van het concept, de collectie of het schema."@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:datatype      rdf:langString ;
    sh:languageIn    ( "nl" "en" ) ;
    sh:maxCount      2 ;
    sh:nodeKind      sh:Literal ;
    sh:path          skos:definition .

beo_ps:note
    a                sh:PropertyShape ;
    rdfs:label       "note"@en ;
    sh:name          "note"@en ;
    sh:description   "Een uitspraak over de betekenis van het concept, de collectie of het schema."@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:datatype      rdf:langString ;
    sh:languageIn    ( "nl" "en" ) ;
    sh:nodeKind      sh:Literal ;
    sh:path          skos:note .

beo_ps:prefLabel
    a                sh:PropertyShape ;
    rdfs:label       "voorkeurslabel"@en ;
    sh:name          "voorkeurslabel"@en ;
    sh:description   "Het voorkeurslabel in de Nederlandse taal. Slechts 1 voorkeurslabel is toegelaten."@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:datatype      rdf:langString ;
    sh:languageIn    ( "nl" ) ;
    sh:maxCount      1 ;
    sh:minCount      1 ;
    sh:nodeKind      sh:Literal ;
    sh:path          skos:prefLabel .

beo_ps:dc_identifier
    a                sh:PropertyShape ;
    rdfs:label       "identifier"@en ;
    sh:name          "identifier"@en ;
    sh:description   "Een globaal unieke identificator."@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:datatype      xsd:string ;
    sh:maxCount      1 ;
    sh:nodeKind      sh:Literal ;
    sh:path          dc:identifier .

beo_ps:conceptscheme_prefLabel
    a                sh:PropertyShape ;
    rdfs:label       "voorkeurslabel"@en ;
    sh:name          "voorkeurslabel"@en ;
    sh:description   "Het voorkeurslabel."@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:datatype      rdf:langString ;
    sh:languageIn    ( "en" "nl" ) ;
    sh:uniqueLang true ;
    sh:maxCount      2 ;
    sh:minCount      1 ;
    sh:nodeKind      sh:Literal ;
    sh:path          skos:prefLabel .

beo_ps:inScheme
    a                sh:PropertyShape ;
    rdfs:label       "is in schema"@en ;
    sh:name          "is in schema"@en ;
    sh:description   "Drukt de relatie uit tussen een 'resource' en het schema dat het bevat."@en ;
    rdfs:isDefinedBy beo_ont:opslaglocatie, beo_voc:opslaglocatie ;
    sh:class         skos:ConceptScheme ;
    sh:maxCount      1 ;
    sh:minCount      1 ;
    sh:nodeKind      sh:IRI ;
    sh:path          skos:inScheme .